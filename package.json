  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/estree": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^20.17.57",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
