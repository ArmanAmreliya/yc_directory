{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type": "module",
    "predev": "npm run typegen",
